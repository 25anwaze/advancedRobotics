#include <Servo.h>
Servo myServo;

const int buttonPin = 11;
const int buttonPin2 = 12;
int buttonState = 0;
int buttonState2 = 0;

void setup () {
 pinMode(buttonPin,INPUT);
 pinMode(buttonPin2,INPUT);
 myServo.attach(9);
}
void loop () {
 buttonState = digitalRead(buttonPin) ;
 buttonState2 = digitalRead(buttonPin2) ;
  
  if (buttonState == HIGH && buttonState2 == HIGH){
 myServo.write(180);
 delay(2000);
  } else  {
 myServo.write(0);
 delay(2000);
  }}
